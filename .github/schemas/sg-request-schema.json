{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": ["request_id", "overall_business_justification", "requests"],
    "properties": {
      "request_id": { "type": "string" },
      "overall_business_justification": { "type": "string" },
      "requests": {
        "type": "array",
        "items": {
          "type": "object",
          "required": [
            "business_justification",
            "thirdparty_name",
            "thirdparty_id",
            "account_id",
            "vpc_id",
            "region"
          ],
          "properties": {
            "business_justification": { "type": "string" },
            "thirdparty_name": { "type": "string" },
            "thirdparty_id": { "type": "string" },
            "account_id": { "type": "string" },
            "vpc_id": { "type": "string" },
            "region": { "type": "string" }
          }
        }
      }
    }
  }
  